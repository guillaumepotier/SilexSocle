        <div class="container">
            {% include "Default/flash_messages.html.twig" %}

            {% block maincontent %}{% endblock %}
            {% block footer %}
                <hr>
                <footer>
                  <p>&copy; <a href="http://twitter.com/guillaumepotier">guillaumepotier</a> 2012</p>
                </footer>
            {% endblock %}
        </div> <!-- /container -->


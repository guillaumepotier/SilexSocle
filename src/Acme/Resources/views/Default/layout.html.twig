<html>
    <head>
        <title>{% block title %}SilexSocle{% endblock %}</title>
        <link href="{{ asset("/twitter/docs/assets/css/bootstrap.css") }}" rel="stylesheet">
        <style type="text/css">
        body {
            padding-top: 60px;
            padding-bottom: 40px;
        }
        input[class*="span"], select[class*="span"] {
            height: 28px;
            margin-bottom: 0;
        }
        p.pull-right {
            color: white;
            text-decoration: none;
            padding: 10px 10px 11px;
            line-height: 19px;
            margin: 0;
        }
        h1, h2 {
            margin-bottom: 20px;
        }
        thead {
            font-weight: bold;
            border-bottom: 2px solid #484848;
        }
        #forkme {
            position: absolute;
            top: 0;
            left: 0;
            z-index: 99999;
        }
        </style>
        <link href="{{ asset("/twitter/docs/assets/css/bootstrap-responsive.css") }}" rel="stylesheet">
    </head>
    <body>
        <!-- <a href="https://github.com/guillaumepotier/Employness">
            <img id="forkme" src="{{ asset("/images/forkme.png") }}" alt="Fork me on GitHub">
        </a> -->
